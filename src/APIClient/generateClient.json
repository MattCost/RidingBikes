{
    "runtime": "Net70",
    "documentGenerator": {
        "fromDocument": {
            "json": "../API/swagger.json"
        }
    },
    "codeGenerators": {
        "openApiToCSharpClient": {
            "output": "ApiClient.g.cs",
            "namespace": "RidingBikes.APIClient",
            "generateClientClasses": true,
            "generateClientInterfaces": true,
            "GenerateDtoTypes" : false,
            "operationGenerationMode": "SingleClientFromOperationId",
            "AdditionalNamespaceUsages": [ "RidingBikes.Common.Models.BikeRouteModels", "RidingBikes.Common.Models.GroupRideModels" ]
            
        }
    }
}